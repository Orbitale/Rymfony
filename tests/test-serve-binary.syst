--RUN-ON-UNIX--
--SETUP--
../Rymfony/target/debug/rymfony stop
--TEST--

../Rymfony/target/debug/rymfony serve -d --document-root $WORKING_DIR/../assetsphp_serve_binary > $WORKING_DIR/server.log 2>&1
sleep 2
curl -v --request GET \
  --cacert $HOME/.rymfony/ca_tls_cert.pem \
  --url https://127.0.0.1:8000/ \
  --header 'Accept: *' \
  --header 'User-Agent: test runner' \
  -o $WORKING_DIR/file.pdf

../Rymfony/target/debug/rymfony stop >/dev/null 2>&1

sha256sum $WORKING_DIR/file.pdf

--EXPECTF--
110b3925e9f381a2304408788f733ead55ad727b466b7ab66ebe46b9b3382d66 .*
